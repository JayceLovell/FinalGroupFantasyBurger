@model int
@{
    ViewBag.Title = "Checkout Complete";
}

<div style="margin-top:165px;">
    <h2>Complete</h2>
    <p>Thanks for your order! Your order number is: @Model</p>
    <p>
        How about trying one of our other Fantastic Burgers?
        @Html.ActionLink("Burgers", "BrowseByCategory", "Menu", new { category = "Burger" }, new { @class = "btn btn-success btn-lg" })
    </p>
    </div>